{
    "name": "my-webpack-project",
    "version": "1.0.0",
    "description": "My webpack project",
    "type": "module",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "webpack  --node-env=production --config webpack.production.config.js",
        "build:dev": "webpack  --config webpack.local.config.js",
        "build:prod": "webpack  --node-env=production --config webpack.production.config.js",
        "watch": "webpack --watch",
        "dev": "webpack serve --node-env=development --config webpack.local.config.js",
        "serve-prod": "webpack serve --node-env=production --config webpack.production.config.js",
        "online": "npx http-server ./dist -a localhost -p 7000 -o -c1",
        "check-package": "depcheck",
        "update-package": "npx npm-check-updates -i",
        "lint": "eslint",
        "pack": "docker build -t react-app-image .",
        "analyze-prod": "webpack --node-env=production --config webpack.production.config.js --profile --json > analyze/stats.json && webpack-bundle-analyzer analyze/stats.json "
    },
    "author": "BinhNguyen",
    "license": "ISC",
